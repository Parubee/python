#ord()
print(ord('A'))  # Output: 65 (Unicode code point of 'A')
print(ord('a'))  # Output: 97 (Unicode code point of 'a')
print(ord('ðŸ˜Š')) # Output: 128522 (Unicode code point of 'ðŸ˜Š')

print ("----------------------------------------------------")

#chr()
print(chr(65))   # Output: 'A'
print(chr(97))   # Output: 'a'
print(chr(128522)) # Output: 'ðŸ˜Š'
